{
  "name": "whistle",
  "version": "1.0.6",
  "private": true,
  "scripts": {
    "start": "npm run watch && npm run watch:preload",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "watch": "vue-cli-service build --watch",
    "build:preload": "rollup -c rollup.config.js",
    "watch:preload": "rollup -c rollup.config.js --watch",
    "watch:webui": "npm run update:webui && webpack --config ./whistle/biz/webui/htdocs/src/webpack.config -w",
    "update:webui": "rm -rf whistle && git clone https://github.com/avwo/whistle.git whistle;",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "ant-design-vue": "^1.5.3",
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "fibers": "^4.0.2",
    "fix-path": "^3.0.0",
    "jquery": "^3.4.1",
    "js-base64": "^2.5.2",
    "lodash": "^4.17.15",
    "lottie-web": "^5.6.6",
    "qrcode": "^1.4.4",
    "qs": "^6.9.1",
    "spawn-sync": "^2.0.0",
    "utools-helper": "^0.1.6",
    "vue": "^2.6.11",
    "vue-router": "^3.1.5",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-conventional": "^7.6.0",
    "@rollup/plugin-multi-entry": "^3.0.0",
    "@types/ip": "^1.1.0",
    "@types/js-base64": "^2.3.1",
    "@types/node": "^12.12.31",
    "@types/qrcode": "^1.3.4",
    "@vue/cli-plugin-babel": "~4.3.1",
    "@vue/cli-plugin-eslint": "~4.3.1",
    "@vue/cli-plugin-router": "~4.3.1",
    "@vue/cli-plugin-vuex": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-import": "^1.13.0",
    "babel-preset-latest": "^6.24.1",
    "copy-webpack-plugin": "^5.1.1",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.1.2",
    "husky": "^1.3.1",
    "ip": "^1.1.5",
    "less": "^2.7.3",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
